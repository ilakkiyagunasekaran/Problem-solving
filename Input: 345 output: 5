import java.util.*;
public class Solution
{
    public static void main(String[] args)
    {
        int max=0;
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        while(n!=0)
        {
            int temp=n%10;
            if(temp>max)
            {
                max=temp;
            }
            n=n/10;
        }
        System.out.print(max);
    }
}
