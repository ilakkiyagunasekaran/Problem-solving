import java.util.*;
public class Solution
{
    public static void main(String[] args)
    {
        int temp;
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        a=a-b;
        b=a+b;
        a=b-a;
        System.out.print(a+" ");
    System.out.print(b);
    }
    
}
